#guessing game

import random

answer=random.randint(0,9)
check=10
count=0
while(check!=answer):
    check=int(input("Enter a guess from 0 to 9: "))
    count+=1
    if check<answer:
        print("Guess too low")
    elif check>answer:
        print("Guess too high")
    else:
        print("You guessed correctly in "+str(count)+" guesses!")
